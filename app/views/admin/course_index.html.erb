<div class="container">
<ul class="nav nav-tabs" role="tablist">
  <li><%= link_to 'Students', admin_students_path %></li>
  <li><%= link_to 'Teacher', admin_teachers_path %></li>
  <li class="active"><%= link_to "Courses", admin_courses_path%></li>
  <li><%= link_to "Classes", admin_classes_path%></li>
  </ul>
  	<h1>Admin management panel</h1><span class="glyphicon glyphicon-heart"></span>
  	  	<%= form_tag admin_courses_path, :method => 'get' do %>
  		<p>
    	<%= text_field_tag :search, params[:search] %>
    	<%= submit_tag "Search", :name => nil, class:'btn btn-primary' %>
  		</p>
		<% end %>
		<table class="table table-hover" style="width:100%" border="1" margin="center">
		<tr>
		  <td><b>Tên học phần</b></td>
 		  <td><b>Mã học phần</b></td>
      <td><b>Trọng số</b></td>
 		  <td><b>Tools</b></td>
 		</tr>
	<%@subjects.each do |s|%>
	 <%=render s%>
	<%end%>
		</table>
		<button type="button" class="btn btn-primary btn-lg" data-toggle="modal" data-target="#courseModal">
      Thêm học phần
    </button> hoặc
    <%=render 'sheet_form', path: admin_upload_courses_path%>
</div>
<%= render 'subjects/form'%>