<div class="container-fluid">
<h1>Nhập điểm</h1>
	<div class="row">
		<div class="col-sm-3 col-sm-2 sidebar">
			<%=render 'teachers/sidebar' %>
		</div>
		<div class="col-sm-9 main">
			<table class="table table-hover" style="width:100%" border="1" margin="center">
			 <tr>
				  <td><b>Sinh viên</b></td>
				  <td><b>MSSV</b></td>
			  	<td><b>Điểm quá trình</b></td>
				  <td><b>Điểm cuối kì</b></td>
				  <td><b>Điểm trung bình</b></td>
				</tr>
				<%=form_tag sclass_update_point_path(@sclass), method: 'POST' do%>
					<%@learnings.each do |learning|%>
				 		<%=render learning%>
					<%end%>
			</table>
					<%=submit_tag 'Update', class: 'btn btn-primary'%>
				<%end%>
		</div>
	</div>
</div>