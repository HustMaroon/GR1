<div id="student-<%=student.id%>">
	<%=index+1%>. <%=link_to student.name, student %> 
	<%if current_user.class == Teacher %>
		<%=link_to sclass_group_remove_group_member_path(@sclass, @group, student),method: 'post' do%>
			<i class="fa fa-times" aria-hidden="true"></i>
		<%end%>
	<%end%>
</div>